                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>/* Transferencias de materiales (Entradas, Proceso y Salidas)*/
/*DECLARE @PEDIDO VARCHAR(10) = '58412';*/


SELECT (
	SELECT TOP (1) Cte.Nombre FROM Venta AS Venta INNER JOIN Cte 
	ON Venta.Cliente = Cte.Cliente WHERE (Venta.Mov = 'PEDIDO') 
	AND (Venta.MovID = @PEDIDO) AND (Cte.Nombre &lt;&gt; 'EXCEL NOBLEZA, S.A. DE C.V.')
) AS Cliente,
/*Obtengo Nombre del cliente*/

Inv.Mov,
Inv.MovID,
InvD.DescripcionExtra,
Inv.Referencia AS Pedido,
Inv.Estatus AS Estado,
Inv.Almacen AS AlmacenOrig,
Inv.AlmacenDestino AS AlmacenDest,
Inv.Usuario,
Inv.FechaConclusion,
InvD.Articulo,
CASE 
	WHEN Art.Grupo in ('MP-Solventes') then 0
	WHEN Art.Grupo in ('MP-Tintas', 'MP-Barniz') then 0.34
	ELSE 1 
END AS Componentes,
InvD.Cantidad * CASE WHEN InvD.Articulo IN ('MPZI-PE7032', 'MPZI-PE7120') THEN 16.3	ELSE InvD.Factor END as Cantidad,
/* Obtengo la conversion a KG, Se  modifica para considerar la conversion a Kg del zipper MPZI-PE7032 , estos se tendra que cambiar al cambiar el factor en Intelisis*/
CASE WHEN InvD.Articulo IN ('MPZI-PE7032', 'MPZI-PE7120') THEN 'KG' ELSE UPPER(InvD.Unidad) END AS Unidad, 
/*Obtengo la Unidad del Articulo*/
InvD.Factor,
Art.Descripcion1, 
Art.Grupo, 
Art.Categoria, 
Art.Familia, 
/*Obtengo tipo de Movimiento*/
CASE 
	WHEN (Inv.Almacen In('MATPRIMA','MP-PENAVE7','TERMINADO') OR (Inv.Almacen = 'PT-PPXTRUS' AND Inv.AlmacenDestino &lt;&gt; 'TERMINADO')OR (Inv.Almacen = 'PT-PEXTRUS' AND Inv.AlmacenDestino &lt;&gt; 'TERMINADO')OR (Inv.Almacen = 'MP-PEXTRUS' AND LEFT(Inv.AlmacenDestino, 2) = 'MP') OR (Inv.Almacen = 'MP-PPXTRUS' AND LEFT(Inv.AlmacenDestino, 2) = 'MP') OR Art.Grupo IN('MP-Tintas','MP-Adh Impr','MP-Solventes','Mp-Barniz')) AND Art.Grupo NOT IN ('ME-Carton') THEN '1.ENTRADA' 
	WHEN (Inv.Almacen = 'MP-PEXTRUS' AND Inv.AlmacenDestino = 'PP-PEXTRUS') THEN 'z_PROCESO_PEXTRUS' 
	WHEN (Inv.Almacen = 'MP-PPXTRUS' AND Inv.AlmacenDestino = 'PP-PPXTRUS') THEN 'z_PROCESO_PPXTRUS' 
	WHEN Inv.AlmacenDestino IN('RECHAZADOS','MATPRIMA','RCALMACEN','RC-CALIDAD','RE-PEXTRUS','RC-PEXTRUS','MP-PENAVE7','RE-PPXTRUS','RC-PPXTRUS','MP-EXTRU') OR (Inv.AlmacenDestino = 'TERMINADO' AND Inv.Almacen NOT IN ('RECHAZADOS'))OR (Inv.AlmacenDestino = 'PT-PPXTRUS' AND (Inv.Almacen NOT IN('PP-PPXTRUS','MP-PPXTRUS')))OR (Inv.AlmacenDestino = 'PT-PEXTRUS' AND (Inv.Almacen NOT IN('PP-PEXTRUS','MP-PEXTRUS')))  THEN '4.SALIDA' 
	ELSE '3.PROCESO' END AS Tipo,
/*Obtengo Categoria de Movimientos*/
CASE 
	WHEN Inv.almacenDestino = 'TERMINADO' THEN 'TERMINADO' 
	WHEN Inv.AlmacenDestino = 'RECHAZADOS' THEN 'RECHAZADO' 
	WHEN Inv.AlmacenDestino IN ('MATPRIMA','PT-PEXTRUS' ,'MP-PENAVE7')OR (Inv.AlmacenDestino = 'PT-PPXTRUS' AND (Inv.Almacen &lt;&gt; 'PP-PPXTRUS' OR Inv.Almacen &lt;&gt; 'MP-PPXTRUS')) THEN 'DEVOLUCION' 
	WHEN Inv.AlmacenDestino IN ( 'RCALMACEN','RC-CALIDAD') THEN 'DESPERDICIOS' 
	WHEN Art.Categoria IN ('ME-Material Empaque') THEN 'MATERIAL DE EMPAQUE'
	WHEN Inv.AlmacenDestino IN('RECHAZADOS', 'RE-PEXTRUS' ,'RE-PPXTRUS','RC-PPXTRUS','RC-PEXTRUS') THEN 'RECHAZADOS'
	WHEN Inv.Almacen IN ('MATPRIMA','PT-PEXTRUS' ,'PT-PPXTRUS','MP-PENAVE7' )OR (Inv.Almacen IN ('MP-PEXTRUS','MP-PPXTRUS') AND LEFT(Inv.AlmacenDestino, 2) = 'MP') THEN 'MATERIA PRIMA'
	WHEN Inv.Almacen = 'TINTAS' or Art.Grupo IN ('MP-Tintas','MP-Solventes','Mp-Barniz') THEN 'TINTAS' 
	WHEN Art.Grupo = 'MP-Adh Impr' THEN 'ADHESIVOS' END AS TipoSalida
FROM Inv AS Inv INNER JOIN InvD ON Inv.ID = InvD.ID INNER JOIN Art ON InvD.Articulo = Art.Articulo
WHERE (Inv.Empresa = 'GRAL') AND (Inv.Referencia LIKE '%' + @PEDIDO)AND (Inv.Mov = 'Transferencia')  AND (Inv.Estatus = 'CONCLUIDO') AND (RTRIM(UPPER(InvD.Unidad)) in ('KG','ROLLOS'))


UNION /*Solicitado en el Pedido*/

SELECT Cte_1.Nombre AS Cliente, 
Venta.Mov, 
Venta.MovID,
VentaD.DescripcionExtra, 
Venta.Referencia, 
'CONCLUIDO', 
Art_1.Linea, 
'' AS AlmacenDest, 
'ContMarg' AS Usuario,
CURRENT_TIMESTAMP AS FechaConclusion, 
Art_1.Articulo, 
1 AS Componentes, 
VentaD.Cantidad, 
UPPER(VentaD.Unidad), 
(select Top 1 AU.[Factor] from ArtUnidad AU where AU.Articulo= Art_1.Articulo and AU.Unidad= VentaD.Unidad),
/*Art_1.Factor, */
Art_1.Descripcion1, 
Art_1.Grupo, 
Art_1.Categoria, 
Art_1.Familia,
'(_Solicitado)' AS Tipo, 
' ' AS TipoSalida 
FROM Venta AS Venta INNER JOIN Cte AS Cte_1 ON Venta.Cliente = Cte_1.Cliente INNER JOIN VentaD ON Venta.ID = VentaD.ID INNER JOIN Art AS Art_1 ON Art_1.Articulo = VentaD.Articulo
/*ArtUnidad AU ON AU.Articulo = Art_1.Articulo &amp; AU.Unidad = VentaD.Unidad*/
WHERE (Venta.Mov = 'PEDIDO') AND (Venta.MovID = @PEDIDO) AND (Cte_1.Nombre &lt;&gt; 'EXCEL NOBLEZA, S.A. DE C.V.') AND (Venta.Estatus &lt;&gt; 'CANCELADO')

UNION /*Obtengo Materiales a MP- de las Odenes de Transferencia*/
SELECT (
	SELECT TOP (1) Cte_2.Nombre 
	FROM Venta AS Venta INNER JOIN Cte AS Cte_2 ON Venta.Cliente = Cte_2.Cliente 
	WHERE (Venta.Mov = 'PEDIDO') AND (Venta.MovID = @PEDIDO) AND (Cte_2.Nombre &lt;&gt; 'EXCEL NOBLEZA, S.A. DE C.V.')
) AS Cliente, 
Inv.Mov, 
Inv.MovID, 
Inv.Observaciones,
Inv.Referencia,
Inv.Estatus,
Inv.Almacen, 
Inv.AlmacenDestino, 
Inv.Usuario, 
isnull(Inv.FechaConclusion, 0), 
InvD_1.Articulo, 
1 As Componentes, 
InvD_1.Cantidad, 
UPPER(InvD_1.Unidad), 
InvD_1.Factor, 
Art_2.Descripcion1,
Art_2.Grupo, 
Art_2.Categoria, 
Art_2.Familia, 
'(Requerido)', 
'' AS TipoSalida 
FROM Inv AS Inv INNER JOIN InvD AS InvD_1 ON Inv.ID = InvD_1.ID INNER JOIN Art AS Art_2 ON InvD_1.Articulo = Art_2.Articulo
WHERE (Inv.Empresa = 'GRAL') AND (Inv.Mov = 'Orden Transferencia') AND (Inv.Referencia LIKE '%' + @PEDIDO) AND (Inv.Estatus = 'CONCLUIDO'  or Inv.Estatus = 'Pendiente') AND (Inv.Usuario in ('JOSECG', 'HECTORH'))AND (RTRIM(UPPER(InvD_1.Unidad)) = 'KG') AND (SUBSTRING(Inv.AlmacenDestino, 1, 3) = 'MP-')

UNION 
SELECT (
	SELECT TOP (1) Cte_2.Nombre 
	FROM Venta AS Venta INNER JOIN Cte AS Cte_2 ON Venta.Cliente = Cte_2.Cliente
	WHERE (Venta.Mov = 'PEDIDO') AND (Venta.MovID = @PEDIDO) AND (Cte_2.Nombre &lt;&gt; 'EXCEL NOBLEZA, S.A. DE C.V.')
) AS Cliente, 
Inv.Mov, 
Inv.MovID, 
Inv.Observaciones,
Inv.Referencia, 
Inv.Estatus,
Inv.Almacen, 
Inv.AlmacenDestino, 
Inv.Usuario, 
isnull(Inv.FechaConclusion, 0), 
InvD_1.Articulo, 
1 As Componentes, 
InvD_1.Cantidad, 
UPPER(InvD_1.Unidad), 
InvD_1.Factor, 
Art_2.Descripcion1,
Art_2.Grupo, 
Art_2.Categoria, 
Art_2.Familia, 
'2.CONSUMOS', 
SUBSTRING(Inv.Almacen, 4, LEN( Inv.Almacen )-3) AS TipoSalida
FROM Inv AS Inv INNER JOIN InvD AS InvD_1 ON Inv.ID = InvD_1.ID INNER JOIN Art AS Art_2 ON InvD_1.Articulo = Art_2.Articulo
WHERE (Inv.Empresa = 'GRAL') AND (Inv.Mov = 'Consumo Material') AND (Inv.Referencia LIKE '%' + @PEDIDO) AND (Inv.Estatus = 'CONCLUIDO' or Inv.Estatus = 'Pendiente') AND (RTRIM(UPPER(InvD_1.Unidad)) = 'KG') AND (SUBSTRING(Inv.Almacen, 4, LEN( Inv.Almacen )-3) not in ('PEXTRUS','PPXTRUS'))
ORDER BY Tipo, Inv.MovID</CommandText>
