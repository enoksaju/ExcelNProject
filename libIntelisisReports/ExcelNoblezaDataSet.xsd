            <MainSource>
              <DbSource ConnectionRef="ExcelNoblezaConnectionString (Settings)" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT 
Art.Articulo, art.Grupo, Art.Descripcion1, UPPER(SL.SerieLote) as SerieLote, 
SL.Almacen, SL.Existencia, SL.ExistenciaAlterna, SL.ExistenciaActivoFijo, SL.UltimaEntrada, 
SL.UltimaSalida, CASE 
WHEN Almacen IN ('MATPRIMA', 'TINTAS', 'TERMINADO', 'RECHAZADOS', 'SERVICIOS', 'DEV-CONVER', 'REVISION', 'RCALMACEN', 'MPTRANSITO', 'AVIOS', 'DESPER', '(TRANSITO)') THEN Almacen 
WHEN Almacen IN ('MAQMP', 'MAQPROCESO', 'MAQPT', 'MAQMP2', 'MAQPP2', 'MAQPT2') THEN 'Maquila' 
ELSE SUBSTRING(Almacen, 4, len(Almacen)) END AS Linea, 
DATEDIFF(DAY, ISNULL(SL.UltimaSalida, SL.UltimaEntrada), CURRENT_TIMESTAMP) AS Antigüedad, 
UPPER(LTRIM(RTRIM(Art.Unidad))) AS Unidad 
FROM SerieLote AS SL LEFT OUTER JOIN Art AS Art ON SL.Articulo = Art.Articulo 
WHERE (SL.Almacen &lt;&gt; '(TRANSITO)') AND (SL.Existencia IS NOT NULL) AND 
Art.Grupo not in ('MP-Tintas','Mp-Barniz','ME-Centros','ME-Carton','MP-Solventes','MP-Adh Impr') AND
(CASE WHEN Almacen IN ('MATPRIMA', 'TINTAS', 'TERMINADO', 'RECHAZADOS', 'SERVICIOS') THEN Almacen 
WHEN Almacen IN ('MAQMP', 'MAQPROCESO', 'MAQPT') THEN 'Maquila' 
ELSE SUBSTRING(Almacen, 4, len(Almacen)) END LIKE @Linea)
ORDER BY Almacen, art.Grupo desc, ART.Articulo, SL.Existencia</CommandText>
                    <Parameters>
                      <Parameter AllowDbNull="false" AutogeneratedName="Linea" ColumnName="" DataSourceName="" DataTypeServer="unknown" DbType="AnsiString" Direction="Input" ParameterName="@Linea" Precision="0" Scale="0" Size="1024" SourceColumn="" SourceColumnNullMapping="false" SourceVersion="Current" />
                    </Parameters>
                  </DbCommand>
                </SelectCommand>
              </DbSource>
